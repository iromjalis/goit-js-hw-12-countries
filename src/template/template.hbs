{{#each this}}  

<li>
    <h1>{{name}}</h1>

<div class="js-countryItem">
  <div class="country-info">
  <h3>Capital: {{capital}}</h3>
  <h4>Population: {{population}}</h4>

  <ul><b>Languages:</b>
    {{#each languages}}
    <li>{{this.name}} </li>
    {{/each}}
  </ul>
  </div>
  <img src="{{flag}} " alt="Flag" width="320" height="auto">
</div>
</li>
{{/each }}